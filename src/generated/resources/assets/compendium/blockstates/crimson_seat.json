{
  "multipart": [
    {
      "apply": {
        "model": "compendium:block/crimsonseat"
      }
    },
    {
      "when": {
        "east": "false",
        "north": "false"
      },
      "apply": {
        "model": "compendium:block/crimsonleg",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false",
        "north": "false"
      },
      "apply": {
        "model": "compendium:block/crimsonleg"
      }
    },
    {
      "when": {
        "east": "false",
        "south": "false"
      },
      "apply": {
        "model": "compendium:block/crimsonleg",
        "y": 180
      }
    },
    {
      "when": {
        "west": "false",
        "south": "false"
      },
      "apply": {
        "model": "compendium:block/crimsonleg",
        "y": -90
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "compendium:block/crimsonseat_extension",
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "compendium:block/crimsonseat_extension",
        "y": -90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "compendium:block/crimsonseat_extension",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "compendium:block/crimsonseat_extension",
        "y": 90,
        "uvlock": true
      }
    }
  ]
}