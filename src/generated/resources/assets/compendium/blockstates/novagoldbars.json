{
  "multipart": [
    {
      "apply": {
        "model": "compendium:block/novagoldbars_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "compendium:block/novagoldbars_side"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "compendium:block/novagoldbars_noside"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "compendium:block/novagoldbars_side_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "compendium:block/novagoldbars_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "compendium:block/novagoldbars_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "compendium:block/novagoldbars_noside",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "compendium:block/novagoldbars_side",
        "y": 90
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "compendium:block/novagoldbars_noside_alt"
      }
    }
  ]
}